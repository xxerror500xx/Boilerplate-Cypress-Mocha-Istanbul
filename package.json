{
  "name": "boilerplate-cypress-mocha-istanbul",
  "version": "1.0.0",
  "description": "This project is an expansion on the cypress-unit-build repo.",
  "keywords": [
    "Cypress",
    "integration",
    "unit",
    "mocha",
    "test",
    "example",
    "modules",
    "fizzbuzz",
    "math",
    "print",
    "ES6",
    "Ecma2015",
    "babel",
    "istanbul",
    "nyc",
    "webpack-dev-server"
  ],
  "private": true,
  "scripts": {
    "cypress-open": "cypress open",
    "build": "webpack",
    "serv-dev": "webpack-dev-server --open",
    "test": "nyc --reporter=text-summary mocha --require babel-core/register --recursive",
    "test-watch": "nyc --reporter=text-summary mocha --require babel-core/register --recursive --watch --watch-extensions js test/**/**/*.js",
    "coverage": "nyc --reporter=html --reporter=text-summary mocha --require babel-core/register --recursive"
  },
  "author": "Clifford Minks",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.19",
    "cypress": "^2.1.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.1.1",
    "nyc": "^11.7.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3"
  },
  "dependencies": {}
}
