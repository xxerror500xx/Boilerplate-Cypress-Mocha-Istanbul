{
  "name": "boilerplate-cypress-mocha-istanbul",
  "version": "1.0.0",
  "description": "This project is an expansion on the cypress-unit-build repo.",
  "keywords": [
    "Cypress",
    "integration",
    "unit",
    "mocha",
    "test",
    "example",
    "modules",
    "fizzbuzz",
    "math",
    "print",
    "ES6",
    "Ecma2015",
    "babel",
    "istanbul",
    "nyc",
    "webpack-dev-server"
  ],
  "private": true,
  "scripts": {
    "cypress-open": "cypress open",
    "build": "webpack",
    "serv-dev": "webpack-dev-server --open",
    "test": "nyc --reporter=text-summary mocha --opts ./mocha.opts --recursive",
    "test-watch": "nyc --reporter=text-summary mocha  --opts ./mocha.watch.opts",
    "coverage": "nyc --reporter=text-summary --reporter=text-summary mocha --opts ./mocha.opts --recursive"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xxerror500xx/Boilerplate-Cypress-Mocha-Istanbul.git"
  },
  "author": "Clifford Minks",
  "license": "ISC",
  "devDependencies": {
    "ajv": "^6.4.0",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.19",
    "cypress": "^2.1.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-import": "^2.11.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.1.1",
    "mocha-webpack-notifier-reporter": "^1.0.0-alpha",
    "nyc": "^11.7.1",
    "webpack": "^4.8.1",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-notifier": "^1.6.0"
  },
  "dependencies": {}
}
